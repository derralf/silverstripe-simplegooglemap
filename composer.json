{
    "name": "derralf/silverstripe-simplegooglemap",
    "description": "Add a google map to a page type (not editable via CMS)",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.2",
        "silverstripe/vendor-plugin": "^1.0"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "expose": [
            "icons",
            "javascript",
            "logos"
        ]
    },
  "autoload": {
    "psr-4": {
      "Derralf\\SimpleGoogleMap\\": "src/"
    }
  }
}
